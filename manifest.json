{
    "manifest_version": 3,
    "name": "Enter solution in ChatGPT",
    "version": "1.0.0",
    "description": "This extension solves the problem when chatgpt doesn't want to send your question with press enter",
    "icons": {
        "32": "icon.png",
        "128": "icon.png"
    },
    "content_scripts": [
        {
            "matches": [
                "https://chat.openai.com/*"
            ],
            "js": [
                "content.js"
            ]
        }
    ]
}
